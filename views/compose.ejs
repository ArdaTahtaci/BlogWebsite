<%- include("partials/header")-%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <h4>Title</h4>
        <input class="form-control" type="text" name="postTitle" autocomplete="off">
        <h4>Post</h4>
        <textarea rows="6"class="form-control" type="text" name="postContent" autocomplete="off"></textarea>
        <button class="btn btn-primary" type="submit" name="submit" >Publish</button>
    </div>
    
</form>

<%- include("partials/footer")-%>
